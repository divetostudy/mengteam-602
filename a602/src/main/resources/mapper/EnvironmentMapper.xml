<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.mapper.EnvironmentMapper">

    <insert id="add" parameterType="java.lang.String">
        insert into environment(photo_url) values(#{url})
    </insert>
    <delete id="deletePhoto" parameterType="java.lang.String">
        delete from environment where photo_url = #{url}
    </delete>
    <select id="get" resultType="java.lang.String">
        select photo_url from environment
    </select>
</mapper>
