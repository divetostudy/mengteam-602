<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.mapper.LbtMapper">

    <insert id="addHomeLbt">
        insert into lbt (photo_url,type) values (#{url},1)
    </insert>
    <insert id="addCGLbt">
        insert into lbt (photo_url,type) values (#{url},2)
    </insert>
    <delete id="deleteHomeLbt">
        delete from lbt where photo_url = #{url}
    </delete>
    <select id="getHomeLbt" resultType="java.lang.String">
        select photo_url from lbt where type = 1
    </select>
    <select id="getCGLbt" resultType="java.lang.String">
        select photo_url from lbt where type = 2
    </select>

</mapper>
